{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "sign-up-1",
  "type": "registry:block",
  "title": "Sign Up - 1",
  "description": "A simple registration form.",
  "dependencies": [
    "@clerk/elements"
  ],
  "registryDependencies": [
    "button",
    "card",
    "input",
    "label",
    "spinner"
  ],
  "files": [
    {
      "path": "registry/blocks/sign-up/sign-up-1.tsx",
      "content": "\"use client\";\n\nimport * as Clerk from \"@clerk/elements/common\";\nimport * as SignUp from \"@clerk/elements/sign-up\";\nimport { Button } from \"@/components/ui/button\";\nimport {\n  Card,\n  CardContent,\n  CardDescription,\n  CardFooter,\n  CardHeader,\n  CardTitle,\n} from \"@/components/ui/card\";\nimport { Input } from \"@/components/ui/input\";\nimport { Label } from \"@/components/ui/label\";\nimport { cn } from \"@/lib/utils\";\nimport Link from \"next/link\";\nimport { Spinner } from \"@/components/ui/spinner\";\n\nexport default function SignUp1() {\n  return (\n    <SignUp.Root>\n      <Clerk.Loading>\n        {(isGlobalLoading) => (\n          <>\n            <SignUp.Step name=\"start\">\n              <Card className=\"w-full sm:w-96 p-3\">\n                <CardContent className=\"flex flex-col gap-y-6 p-5\">\n                  <div className=\"flex flex-col gap-y-2\">\n                    <CardTitle>Acme, Inc.</CardTitle>\n                    <CardDescription>\n                      Please fill out the form to create your account.\n                    </CardDescription>\n                  </div>\n\n                  <div className=\"flex flex-col gap-y-3\">\n                    <div className=\"grid grid-cols-2 gap-x-3\">\n                      <Clerk.Field name=\"firstName\" className=\"space-y-2\">\n                        <Clerk.Label asChild>\n                          <Label>First Name</Label>\n                        </Clerk.Label>\n                        <Clerk.Input\n                          type=\"text\"\n                          autoComplete=\"given-name\"\n                          placeholder=\"Robert\"\n                          required\n                          asChild\n                        >\n                          <Input name=\"firstName\" autoComplete=\"given-name\" />\n                        </Clerk.Input>\n                        <Clerk.FieldError className=\"block text-sm text-destructive\" />\n                      </Clerk.Field>\n                      <Clerk.Field name=\"lastName\" className=\"space-y-2\">\n                        <Clerk.Label asChild>\n                          <Label>Last Name</Label>\n                        </Clerk.Label>\n                        <Clerk.Input\n                          type=\"text\"\n                          placeholder=\"Kinney\"\n                          autoComplete=\"family-name\"\n                          required\n                          asChild\n                        >\n                          <Input name=\"lastName\" autoComplete=\"family-name\" />\n                        </Clerk.Input>\n                        <Clerk.FieldError className=\"block text-sm text-destructive\" />\n                      </Clerk.Field>\n                    </div>\n                    <Clerk.Field name=\"emailAddress\" className=\"space-y-2\">\n                      <Clerk.Label asChild>\n                        <Label>Email</Label>\n                      </Clerk.Label>\n                      <Clerk.Input type=\"email\" required asChild>\n                        <Input placeholder=\"robertskinney@outlook.com\" />\n                      </Clerk.Input>\n                      <Clerk.FieldError className=\"block text-sm text-destructive\" />\n                    </Clerk.Field>\n                    <Clerk.Field name=\"password\" className=\"space-y-2\">\n                      <Clerk.Label asChild>\n                        <Label>Password</Label>\n                      </Clerk.Label>\n                      <Clerk.Input type=\"password\" required asChild>\n                        <Input placeholder=\"*******\" />\n                      </Clerk.Input>\n                      <Clerk.FieldError className=\"block text-sm text-destructive\" />\n                    </Clerk.Field>\n                  </div>\n                </CardContent>\n                <CardFooter className=\"flex flex-col w-full gap-y-4\">\n                  <SignUp.Captcha className=\"empty:hidden\" />\n                  <SignUp.Action submit asChild>\n                    <Button className=\"w-full\" disabled={isGlobalLoading}>\n                      <Clerk.Loading>\n                        {(isLoading) => {\n                          return isLoading ? <Spinner /> : \"Continue\";\n                        }}\n                      </Clerk.Loading>\n                    </Button>\n                  </SignUp.Action>\n                  <Button variant=\"link\" size=\"sm\" asChild>\n                    <Link\n                      href=\"/sign-in\"\n                      className=\"text-primary dark:text-secondary-foreground w-full text-wrap\"\n                    >\n                      Already have an account? Sign in\n                    </Link>\n                  </Button>\n                </CardFooter>\n              </Card>\n            </SignUp.Step>\n\n            <SignUp.Step name=\"continue\">\n              <Card className=\"w-full sm:w-96\">\n                <CardHeader>\n                  <CardTitle>Continue registration</CardTitle>\n                </CardHeader>\n                <CardContent>\n                  <Clerk.Field name=\"username\" className=\"space-y-2\">\n                    <Clerk.Label>\n                      <Label>Username</Label>\n                    </Clerk.Label>\n                    <Clerk.Input type=\"text\" required asChild>\n                      <Input />\n                    </Clerk.Input>\n                    <Clerk.FieldError className=\"block text-sm text-destructive\" />\n                  </Clerk.Field>\n                </CardContent>\n                <CardFooter>\n                  <div className=\"grid w-full gap-y-4\">\n                    <SignUp.Action submit asChild>\n                      <Button disabled={isGlobalLoading}>\n                        <Clerk.Loading>\n                          {(isLoading) => {\n                            return isLoading ? <Spinner /> : \"Continue\";\n                          }}\n                        </Clerk.Loading>\n                      </Button>\n                    </SignUp.Action>\n                  </div>\n                </CardFooter>\n              </Card>\n            </SignUp.Step>\n\n            <SignUp.Step name=\"verifications\">\n              <SignUp.Strategy name=\"email_code\">\n                <Card className=\"w-full sm:w-96\">\n                  <CardHeader>\n                    <CardTitle>Verify your email</CardTitle>\n                    <CardDescription>\n                      Use the verification link sent to your email address\n                    </CardDescription>\n                  </CardHeader>\n                  <CardContent className=\"grid gap-y-4\">\n                    <div className=\"grid items-center justify-center gap-y-2\">\n                      <Clerk.Field name=\"code\" className=\"space-y-2\">\n                        <Clerk.Label className=\"sr-only\">\n                          Email address\n                        </Clerk.Label>\n                        <div className=\"flex justify-center text-center\">\n                          <Clerk.Input\n                            type=\"otp\"\n                            className=\"flex justify-center has-disabled:opacity-50\"\n                            autoSubmit\n                            render={({ value, status }) => {\n                              return (\n                                <div\n                                  data-status={status}\n                                  className={cn(\n                                    \"relative flex size-10 items-center justify-center border-y border-r border-input text-sm transition-all first:rounded-l-md first:border-l last:rounded-r-md\",\n                                    {\n                                      \"z-10 ring-2 ring-ring ring-offset-background\":\n                                        status === \"cursor\" ||\n                                        status === \"selected\",\n                                    }\n                                  )}\n                                >\n                                  {value}\n                                  {status === \"cursor\" && (\n                                    <div className=\"pointer-events-none absolute inset-0 flex items-center justify-center\">\n                                      <div className=\"animate-caret-blink h-4 w-px bg-foreground duration-1000\" />\n                                    </div>\n                                  )}\n                                </div>\n                              );\n                            }}\n                          />\n                        </div>\n                        <Clerk.FieldError className=\"block text-center text-sm text-destructive\" />\n                      </Clerk.Field>\n                      <SignUp.Action\n                        asChild\n                        resend\n                        className=\"text-muted-foreground\"\n                        fallback={({ resendableAfter }) => (\n                          <Button variant=\"link\" size=\"sm\" disabled>\n                            Didn&apos;t receive a code? Resend (\n                            <span className=\"tabular-nums\">\n                              {resendableAfter}\n                            </span>\n                            )\n                          </Button>\n                        )}\n                      >\n                        <Button type=\"button\" variant=\"link\" size=\"sm\">\n                          Didn&apos;t receive a code? Resend\n                        </Button>\n                      </SignUp.Action>\n                    </div>\n                  </CardContent>\n                  <CardFooter>\n                    <div className=\"grid w-full gap-y-4\">\n                      <SignUp.Action submit asChild>\n                        <Button disabled={isGlobalLoading}>\n                          <Clerk.Loading>\n                            {(isLoading) => {\n                              return isLoading ? <Spinner /> : \"Continue\";\n                            }}\n                          </Clerk.Loading>\n                        </Button>\n                      </SignUp.Action>\n                    </div>\n                  </CardFooter>\n                </Card>\n              </SignUp.Strategy>\n            </SignUp.Step>\n\n            <div id=\"clerk-captcha\" />\n          </>\n        )}\n      </Clerk.Loading>\n    </SignUp.Root>\n  );\n}\n",
      "type": "registry:component"
    }
  ]
}